{
  "name": "fun-writing-backend",
  "version": "1.0.0",
  "description": "Fun Writing - Gamified Creative Writing Platform Backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": ["writing", "ai", "education"],
  "author": "Victor",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "google-auth-library": "^9.4.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "uuid": "^9.0.1",
    "@google-cloud/storage": "^7.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
